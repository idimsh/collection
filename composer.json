{
  "name": "idimsh/collection",
  "type": "library",
  "description": "A generic objects/variables holder and iterator",
  "keywords": [
    "collection",
    "iterator",
    "PHP",
    "list"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abdulrahman Dimashki",
      "email": "idimsh@gmail.com"
    }
  ],
  "homepage": "https://github.com/idimsh/collection",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "squizlabs/php_codesniffer": "~3.2",
    "symfony/yaml": "~2.1|~3.0",
    "doctrine/instantiator": "1.0.5"
  },
  "autoload": {
    "psr-4": {
      "Dimsh\\Models\\Collections\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test/TestBasicObject.php",
      "test/TestBasicObjectsList.php"
    ]
  },
  "scripts": {
    "test": [
      "phpunit test/",
      "phpcs -p src"
    ]
  },
  "scripts-descriptions": {
    "test": "Run all tests and quality checks"
  }
}
